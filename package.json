{
    "name": "dom5-server-manager",
    "version": "1.0.0",
    "description": "Explorer Backend, API & mongodb database",
    "main": "build/index.js",
    "repository": "git@github.com:PiJaAB/explorer-backend.git",
    "license": "MIT",
    "private": true,
    "scripts": {
        "start": "cross-env NODE_ENV=production node ./build/index.js",
        "build": "cross-env NODE_ENV=production run-p -l --silent --aggregate-output build:* && run-s -l --silent build:typescript:*",
        "build:next": "next build ./site",
        "build:typescript": "rimraf build",
        "build:typescript:tsc": "tsc",
        "dev": "nodemon -w server -w types -w .env -- ./server/index.ts",
        "lint": "run-p -l --silent --aggregate-output --continue-on-error lint:*",
        "lint:eslint-check": "eslint --print-config 'src/**/*.js' | eslint-config-prettier-check",
        "lint:prettier": "prettier -c 'src/**/*'",
        "lint:eslint": "eslint 'src/**/*.{js,ts}' 'types/**/*.{js,ts}'",
        "lint:typecheck": "tsc --noEmit --pretty"
    },
    "nodemonConfig": {
        "ext": "js,json,ts"
    },
    "dependencies": {
        "argon2": "^0.27.0",
        "axios": "^0.20.0",
        "better-sqlite3": "^7.1.1",
        "bootstrap": "^4.5.2",
        "chalk": "^4.1.0",
        "classnames": "^2.2.6",
        "cross-env": "^7.0.2",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "mdi-react": "^7.3.0",
        "nat-upnp": "^1.1.1",
        "next": "^9.5.3",
        "next-env": "^1.1.1",
        "nprogress": "^0.2.0",
        "react": "^16.13.1",
        "react-bootstrap": "^1.3.0",
        "react-bootstrap-switch": "^15.5.3",
        "react-dom": "^16.13.1"
    },
    "devDependencies": {
        "@tsconfig/node12": "^1.0.7",
        "@types/better-sqlite3": "^5.4.0",
        "@types/classnames": "^2.2.10",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.8",
        "@types/nat-upnp": "^1.1.0",
        "@types/nprogress": "^0.2.0",
        "@types/react": "^16.9.49",
        "@typescript-eslint/eslint-plugin": "^4.1.0",
        "@typescript-eslint/parser": "^4.1.0",
        "eslint": "^7.8.1",
        "eslint-config-airbnb": "^18.2.0",
        "eslint-config-airbnb-base": "^14.2.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.20.6",
        "eslint-plugin-react-hooks": "^4.1.2",
        "nodemon": "^2.0.4",
        "npm-run-all": "^4.1.5",
        "npx": "^10.2.2",
        "prettier": "^2.1.1",
        "rimraf": "^3.0.2",
        "sass": "^1.26.10",
        "ts-node": "^9.0.0",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^4.0.2"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "engines": {
        "node": "^12 || >=13.7"
    }
}